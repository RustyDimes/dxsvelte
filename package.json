{
  "name": "dxsvelte",
  "version": "0.1.0",
  "description": "Bring the power of Svelte to your Django projects.",
  "main": "dist/dxsvelte-init.js",
  "bin": {
    "dxsvelte": "./dist/dxsvelte-init.js"
  },
  "type": "module",
  "scripts": {
    "start": "node ./dist/dxsvelte-init.js",
    "compile": "node ./dist/dxsvelte-compiler.js",
    "build-dist": "ts-node ./src/build",
    "build-dist-and-update-playground": "ts-node ./src/build && node ./update-playground.js"
  },
  "engines": {
    "node": ">=16.10.0"
  },
  "author": "emmyarty",
  "license": "MIT",
  "dependencies": {
    "@types/node": "^18.14.6",
    "esbuild": "0.17.11",
    "esbuild-plugin-inline-import": "^1.0.1",
    "esbuild-plugin-postcss": "^0.1.4",
    "esbuild-svelte": "^0.7.3",
    "js-base64": "^3.7.5",
    "svelte": "^3.56.0",
    "svelte-preprocess": "^5.0.3"
  },
  "keywords": [
    "svelte",
    "django",
    "SPA",
    "SSR",
    "CSR",
    "client-side rendering",
    "server-side rendering",
    "esbuild"
  ]
}
